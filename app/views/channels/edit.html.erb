<div class="row">
  <div class="col-sm-6">
    <h2>Edit <%= @channel.channel_name %></h2>

    <%= simple_form_for @channel do |c| %>
      <%= c.error_notification %>
      <%= c.input :channel_name, placeholder: 'Channel' %>
      <%= c.button :submit, {:class => "btn btn-secondary"} %>
      <%= link_to "Cancel", :back, {:class => "btn btn-default"} %>
    <% end %>
  </div>
</div>