<div class="row">
  <div class="col-sm-6">
    <h1>New Channel</h1>
    <%=  simple_form_for @channel do |f| %>
      <%=  f.error_notification %>
      <%= f.input :channel_name, placeholder: 'Channel' %>
      <%= f.button :submit, {:class=>"btn btn-secondary"} %>
      <%= link_to "Cancel", :back, {:class=>"btn btn-default"} %>
    <% end %>
  </div>
</div>